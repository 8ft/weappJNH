page {
  background: #fff;
}
.switch {
  padding-bottom: 17rpx;
  background: #fff;
  text-align: center;
  border-bottom: 1rpx solid rgba(229, 229, 229, 0.5);
}
.switch view {
  width: 200rpx;
  height: 60rpx;
  display: inline-block;
  line-height: 60rpx;
  border: 1rpx solid #ccc;
  color: #333;
}
.switch view:nth-of-type(1) {
  border-radius: 6rpx 0 0 6rpx;
}
.switch view:nth-of-type(2) {
  border-radius: 0 6rpx 6rpx 0;
}
.switch view.active {
  background: #0083ff;
  border-color: #0083ff;
  color: #fff;
}
