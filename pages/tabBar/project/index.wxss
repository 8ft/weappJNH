page {
  background: #fff;
}
swiper {
  width: 670rpx;
  height: 260rpx;
  margin: 24rpx auto;
  background: #4db2ff;
  border-radius: 10rpx;
}
swiper image {
  border-radius: 10rpx;
  width: 100%;
  height: 100%;
}
.types {
  display: flex;
  padding: 0 10rpx;
}
.types navigator {
  flex: 1;
  text-align: center;
}
.types image {
  width: 90rpx;
  height: 90rpx;
}
.types text {
  display: block;
  font-size: 24rpx;
  color: #666;
  font-weight: 400;
}
.projects {
  padding: 0 40rpx;
}
.title {
  display: flex;
  align-items: center;
  font-size: 36rpx;
  color: #333;
  line-height: 36rpx;
  margin: 62rpx 0 17rpx;
}
.title .iconfont {
  font-size: 36rpx;
  margin-right: 20rpx;
}
.title text {
  font-weight: 500;
}
.title navigator {
  flex: 1;
  text-align: right;
  font-size: 28rpx;
  color: #666;
}
