page {
  background: #fff;
}
.big-title {
  text-indent: 40rpx;
}
.types {
  width: 100%;
  padding: 32rpx 0;
  white-space: nowrap;
  box-sizing: border-box;
}
.type {
  font-size: 28rpx;
  line-height: 60rpx;
  margin-right: 24rpx;
  color: #666;
  background: #f7f7f7;
  padding: 0 36rpx;
  border-radius: 99rpx;
  display: inline-block;
}
.type:first-of-type {
  margin-left: 30rpx;
}
.type.active {
  color: #0083ff;
  background: #e5f2ff;
  font-weight: 500;
}
#projects {
  position: relative;
  width: 100%;
}
.project {
  display: block;
  padding: 44rpx 30rpx 44rpx 134rpx;
  position: relative;
}
.icon {
  position: absolute;
  left: 30rpx;
  top: 44rpx;
  width: 84rpx;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  color: #fff;
  border-radius: 50%;
}
.icon.kaifa {
  background: #f5b94f;
}
.icon.sheji {
  background: #ff907f;
}
.icon.chanpin {
  background: #4db2ff;
}
.icon.yunying {
  background: #9d86eb;
}
.iconfont {
  font-size: 45rpx;
}
.name {
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333;
  margin-bottom: 20rpx;
  font-weight: 500;
  width: 66%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.base-info {
  font-size: 24rpx;
  color: #999;
}
.publish-time {
  margin-left: 41rpx;
}
.price {
  color: #fc5b31;
  font-size: 28rpx;
  line-height: 28rpx;
  position: absolute;
  right: 30rpx;
  top: 44rpx;
  font-weight: 500;
}
view[class^='status'] {
  line-height: 32rpx;
  padding: 0 12rpx;
  color: #fff;
  position: absolute;
  right: 30rpx;
  bottom: 44rpx;
  border-radius: 99rpx;
  font-size: 20rpx;
  font-weight: 400;
}
.status1,
.status10,
.status14 {
  background: #4db2ff;
}
.status2,
.status5,
.status6,
.status7 {
  background: #1ac6a4;
}
.status3,
.status9,
.status13 {
  background: #fc5b31;
}
.status4 {
  background: #f9b337;
}
.status8 {
  background: #95d364;
}
.status11,
.status12 {
  background: #ccc;
}
